{
    "swagger": "2.0",
    "host": "127.0.0.1:8000",
    "basePath": "/api/washing_alots_app/",
    "info": {
        "version": "1.0.0",
        "title": "Simple API",
        "description": "A simple API to learn how to write OpenAPI Specification"
    },
    "schemes": [
        "http"
    ],
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "securityDefinitions": {
        "oauth": {
            "tokenUrl": "http://auth.ibtspl.com/oauth2/",
            "flow": "password",
            "scopes": {
                "read": "read users",
                "write": "create users",
                "update": "update users",
                "delete": "delete users"
            },
            "type": "oauth2"
        }
    },
    "security": [],
    "definitions": {

        "AvailableDate": {
            "type": "object",
            "properties": {
                "AvailableDate": {
                    "type": "string",
                    "format": "date"
                },
                "TimeSlots": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }

        }

    },
    "parameters": {
        "TodoId": {
            "description": "todo id",
            "in": "path",
            "name": "id",
            "required": true,
            "type": "integer"
        }
    },
    "responses": {
        "SuccessResponse": {
            "description": "success response"
        }
    },
    "paths": {
        "/available_slots/": {
            "get": {
                "summary": "get available slots",
                "description": "get available_slots",
                "operationId": "available_slots",
                "security": [{
                    "oauth": [
                        "write",
                        "read"
                    ]
                }],
                "responses": {
                    "200": {
                        "description": "get available_slots",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/AvailableDate"
                            }
                        }
                    }
                }
            }
        },
        "/todos/{id}/": {
            "parameters": [{
                "$ref": "#/parameters/TodoId"
            }],
            "get": {
                "summary": "Get a todo",
                "description": "Returns a todo.",
                "operationId": "get_todo",
                "security": [{
                    "oauth": [
                        "read"
                    ]
                }],
                "responses": {
                    "200": {
                        "description": "A todo",

                    }
                }
            },
            "put": {
                "parameters": [{
                    "$ref": "#/parameters/TodoId"
                }],
                "summary": "Updates a todo",
                "description": "Updates a todo.",
                "operationId": "update_todo",
                "security": [{
                    "oauth": [
                        "update"
                    ]
                }],
                "responses": {
                    "200": {
                        "$ref": "#/responses/SuccessResponse"
                    }
                }
            },
            "delete": {
                "summary": "Deletes a todo",
                "description": "Deletes a todo.",
                "operationId": "delete_todo",
                "security": [{
                    "oauth": [
                        "delete"
                    ]
                }],
                "responses": {
                    "200": {
                        "$ref": "#/responses/SuccessResponse"
                    }
                }
            }
        }
    }
}
